<p id="notice"><%= notice %></p>

<h1>Listing Insurers</h1>

<table>
  <thead>
    <tr>
      <th>ФИО</th>
      <th>Login</th>
      <th>Password</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @insurers.each do |insurer| %>
      <tr>
        <td><%= insurer.fio %></td>
        <td><%= insurer.login %></td>
        <td><%= insurer.password %></td>
        <td><%= link_to 'Show', insurer %></td>
        <td><%= link_to 'Edit', edit_insurer_path(insurer) %></td>
        <td><%= link_to 'Destroy', insurer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Новый страховщик!', new_insurer_path %>
